    "commitizen": {
      "path": "./index.js"
    }
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commitizen": "^4.2.3",
    "conventional-commit-types": "^3.0.0",
    "lodash.map": "^4.6.0",
    "longest": "^2.0.1",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.0",
    "chai": "^4.3.0",
    "cosmiconfig": "^7.0.0",
    "mocha": "^8.3.0",
    "mock-require": "^3.0.3",
    "prettier": "^2.2.1"
  },
  "optionalDependencies": {
    "@commitlint/load": "^11.0.0"
