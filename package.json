{
  "name": "cz-commit-meant",
  "version": "0.0.1",
  "description": "Commitizen adapter following the conventional-changelog format.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha *.test.js",
    "format": "prettier --write *.js"
  },
  "homepage": "https://github.com/wewillcraft/cz-commit-meant",
  "repository": {
    "type": "git",
    "url": "https://github.com/wewillcraft/cz-commit-meant.git"
  },
  "author": {
    "name": "We Will Craft",
    "email": "we@willcraft.software",
    "url": "https://willcraft.software/"
  },
  "contributors": [
    {
      "name": "Jim Cummins",
      "email": "jimthedev@gmail.com"
    }
  ],
  "engineStrict": true,
  "engines": {
    "node": ">= 10"
  },
  "license": "MIT",
